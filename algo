ALGORITHM  solution1
VAR
    tab1;tab2 : ARRAY_of integer;
    sum,i,j : integer;
BEGIN
    FOR i FROM min TO tab1.lengh-1 STEP 1  DO
    FOR j FROM min TO tab2.lengh-1 STEP 1  DO
        IF (tab1[i]<>tab2[j]) THEN
        sum+= tab1[i]
            
        END_IF
    END_FOR
    END_FOR


      FOR  j FROM min TO tab2.lengh-1 STEP 1  DO
    FOR i FROM min TO tab1.lengh-1 STEP 1  DO
        IF (tab2[j]<>tab1[i]) THEN
        sum+= tab2[j]
END_IF
END_FOR
        
    END_FOR
END